										"source": {
											"type": "AzureSqlSource",
											"sqlReaderQuery": {
												"value": "IF OBJECT_ID('dbo.TopNfiles') IS NULL \n    CREATE TABLE dbo.TopNfiles (\n        Itemname VARCHAR(120), \n        lastmoddatetime DATETIME\n    );\n\nINSERT INTO dbo.TopNfiles (Itemname, lastmoddatetime)\nVALUES (\n    '@{activity('Get Metadata2').output.itemName}', \n    '@{formatDateTime(activity('Get Metadata2').output.lastModified, 'yyyy-MM-dd HH:mm:ss')}'\n);\n\nSELECT 1 AS output;\n",
												"type": "Expression"
											},
											"queryTimeout": "02:00:00",
											"partitionOption": "None"
										},
										"dataset": {
